{
	"name": "sfb-web-app",
	"version": "1.0.0",
	"description": "This is where the code of the web app client is, here we are going to code or app and when finished build it for production.",
	"main": "index.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"build-dev": "docker build -t mbi-web-app-dev --file Dockerfile.dev .",
		"up-dev": "docker rm mbi-web-app || true && docker container run -p 4000:4000 --name mbi-web-app --network=mbi-network -ti --mount type=bind,source=\"$(pwd)\",target=/usr/app/src mbi-web-app-dev",
		"start-dev": "docker start -ai mbi-web-app",
		"web-start-dev": "(npm run compile-app-for-server -- -watch --verbose & npm run start-render-server-dev)",
		"start-render-server-dev": "env $(cat env/.dev.env | grep -v ^# | xargs) nodemon ./render-server/server.js",
		"compile-app-for-server": "npx babel ./app --out-dir ./app-build --config-file=./.babelrc.server"
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"koa": "^2.6.2",
		"koa-bodyparser": "^4.2.1",
		"koa-mount": "^4.0.0",
		"koa-router": "^7.4.0",
		"koa-static": "^5.0.0",
		"lodash": "^4.17.11",
		"winston": "^3.1.0",
		"@material-ui/core": "^3.9.2",
		"react-router-dom": "^5.0.0",
		"react-icons": "^3.5.0",
		"react-slick": "^0.23.2"
	},
	"devDependencies": {
		"@babel/cli": "^7.1.5",
		"@babel/core": "^7.1.6",
		"@babel/plugin-proposal-class-properties": "^7.1.0",
		"@babel/plugin-proposal-export-namespace-from": "^7.0.0",
		"@babel/plugin-proposal-function-bind": "^7.0.0",
		"@babel/polyfill": "^7.0.0",
		"@babel/preset-env": "^7.1.6",
		"@babel/preset-react": "^7.0.0",
		"@cerebral/react": "^4.0.3",
		"babel-loader": "^8.0.4",
		"babel-plugin-cerebral": "^1.0.1",
		"babel-plugin-styled-components": "^1.8.0",
		"cerebral": "^5.0.3",
		"koa-webpack-dev-middleware": "^2.0.2",
		"nodemon": "^1.18.10",
		"prettier": "^1.15.2",
		"react": "^16.6.3",
		"react-dom": "^16.6.3",
		"styled-components": "^4.1.1",
		"webpack": "^4.26.0"
	}
}
